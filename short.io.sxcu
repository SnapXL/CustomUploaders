{
  "Version": "18.0.2",
  "Name": "short.io",
  "DestinationType": "URLShortener",
  "RequestMethod": "POST",
  "RequestURL": "https://api.short.io/links",
  "Headers": {
    "Authorization": "YOUR_API_KEY"
  },
  "Body": "JSON",
  "Data": "{\n  \"domain\": \"YOUR_DOMAIN\",\n  \"originalURL\": \"{input}\"\n}",
  "URL": "{json:secureShortURL}",
  "ErrorMessage": "{json:error}"
}