{
  "Version": "18.0.2",
  "Name": "imgchest.com - folder",
  "DestinationType": "ImageUploader",
  "RequestMethod": "POST",
  "RequestURL": "https://api.imgchest.com/v1/post/$ID/add",
  "Headers": {
    "Authorization": "Bearer $TOKEN"
  },
  "Body": "MultipartFormData",
  "FileFormName": "images[]",
  "URL": "{json:data.images[0].link}"
}