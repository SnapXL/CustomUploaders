{
  "Version": "18.0.2",
  "Name": "imgchest.com",
  "DestinationType": "ImageUploader",
  "RequestMethod": "POST",
  "RequestURL": "https://api.imgchest.com/v1/post",
  "Headers": {
    "Authorization": "Bearer PLEASE_ENTER_APIKEY"
  },
  "Body": "MultipartFormData",
  "FileFormName": "images[]",
  "URL": "https://imgchest.com/p/{json:data.id}",
  "ThumbnailURL": "{json:data.images[0].link}",
  "DeletionURL": "{json:data.delete_url}"
}
